{
  "name": "buscatodo",
  "version": "1.0.0",
  "description": "BuscaTodo's scraping tool",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "test": "ava",
    "test:watch": "npm run test -- -w",
    "lint": "eslint ./ ; exit 0",
    "update:test": "./bin/update-test",
    "build": "rimraf build && babel src -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mallendeo/buscatodo-scraper.git"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "files": [
      "test/**/*.js",
      "!**/helpers.js"
    ]
  },
  "author": "mallendeo",
  "bugs": {
    "url": "https://github.com/mallendeo/buscatodo-scraper/issues"
  },
  "homepage": "https://github.com/mallendeo/buscatodo-scraper#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "fs-extra": "^0.30.0",
    "chalk": "^1.1.3"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "cheerio": "^0.20.0",
    "iconv-lite": "^0.4.13",
    "lodash": "^4.15.0"
  }
}
